generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(auto()) @map("_id") @db.ObjectId
  name       String
  password   String
  profile    String
  created_at DateTime? @default(now())
  updated_at DateTime? @default(now())

  @@map("users")
}

model Research {
  id              String    @id @default(auto()) @map("_id") @db.ObjectId
  name1           String
  name2           String
  informations    String
  graphs          String
  created_at      DateTime? @default(now())
  updated_at      DateTime? @default(now())

  @@map("savedResearches")
}